<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xe.api.mapper.BasicMapper">
    <select id="selectUser" resultType="BasicDto">
        SELECT
            name as name
        FROM
            user
        LIMIT 1
    </select>
</mapper>